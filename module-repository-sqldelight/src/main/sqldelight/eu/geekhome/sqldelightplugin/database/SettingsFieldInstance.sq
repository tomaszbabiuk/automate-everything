CREATE TABLE IF NOT EXISTS settingsFieldInstance (
  clazz TEXT NOT NULL,
  name TEXT NOT NULL,
  value TEXT,
  PRIMARY KEY(name, clazz)
);

selectAll:
SELECT *
FROM settingsFieldInstance;

selectByClazz:
SELECT *
FROM settingsFieldInstance
WHERE clazz = ?;

insertOrReplace:
INSERT OR REPLACE INTO settingsFieldInstance(clazz, name, value)
VALUES (?, ?, ?);