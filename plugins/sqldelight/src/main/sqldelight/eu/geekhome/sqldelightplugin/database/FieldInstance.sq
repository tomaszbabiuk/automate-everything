CREATE TABLE IF NOT EXISTS fieldInstance (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  instance_id INTEGER,
  name TEXT NOT NULL,
  value TEXT,
  FOREIGN KEY(instance_id) REFERENCES configurableInstance(id) ON DELETE CASCADE
);

selectAll:
SELECT *
FROM fieldInstance;

selectOfConfigurableInstance:
SELECT *
FROM fieldInstance
WHERE instance_id = ?;

insert:
INSERT INTO fieldInstance(instance_id, name, value)
VALUES (?, ?, ?);