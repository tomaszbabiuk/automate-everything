CREATE TABLE IF NOT EXISTS fieldInstance (
  id INTEGER PRIMARY KEY,
  instance_id INTEGER,
  name TEXT,
  value TEXT,
  FOREIGN KEY(instance_id) REFERENCES configurableInstance(id)
);

selectAll:
SELECT *
FROM fieldInstance;

selectOfConfigurableInstance:
SELECT *
FROM fieldInstance
WHERE instance_id = ?;

insert:
INSERT INTO fieldInstance(id, instance_id, name, value)
VALUES (_rowid_, ?, ?, ?);