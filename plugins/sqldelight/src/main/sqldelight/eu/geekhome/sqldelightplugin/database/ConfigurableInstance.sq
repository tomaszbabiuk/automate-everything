CREATE TABLE IF NOT EXISTS configurableInstance (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  icon_id INTEGER,
  clazz TEXT NOT NULL,

  FOREIGN KEY(icon_id) REFERENCES icon(id)
);

selectAll:
SELECT *
FROM configurableInstance;

selectByClazz:
SELECT *
FROM configurableInstance
WHERE clazz = ?;

selectById:
SELECT *
FROM configurableInstance
WHERE id = ?;

insert:
INSERT INTO configurableInstance(clazz, icon_id)
VALUES (?,?);