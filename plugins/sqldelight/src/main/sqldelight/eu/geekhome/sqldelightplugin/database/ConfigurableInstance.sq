CREATE TABLE IF NOT EXISTS configurableInstance (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  clazz TEXT NOT NULL
);

selectAll:
SELECT *
FROM configurableInstance;

selectByClazz:
SELECT *
FROM configurableInstance
WHERE clazz = ?;

selectById:
SELECT *
FROM configurableInstance
WHERE id = ?;

insert:
INSERT INTO configurableInstance(clazz)
VALUES (?);